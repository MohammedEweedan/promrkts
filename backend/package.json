{
  "name": "bait-altadawul-server",
  "version": "1.0.0",
  "description": "Backend server for BA application",
  "main": "dist/index.js",
  "scripts": {
    "test:email": "node scripts/send-test-email.js",
    "start": "node dist/index.js",
    "dev": "nodemon --exec ts-node src/index.ts",
    "build": "prisma generate && tsc",
    "migrate:deploy": "prisma migrate deploy",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev --name init",
    "test": "jest",
    "load:test": "mkdir -p reports && node -r ./scripts/undici-polyfill.js ./node_modules/.bin/artillery run artillery.yml -o reports/artillery.json",
    "load:report": "artillery report reports/artillery.json -o reports/artillery.html",
    "report:pdf": "ts-node scripts/html-to-pdf.ts reports/artillery.html reports/artillery.pdf",
    "report:all": "npm run load:test && npm run load:report && npm run report:pdf",
    "lint": "eslint . --ext .ts",
    "format": "prettier --write \"src/**/*.ts\"",
    "migrate:up": "ts-node src/config/migrate.ts up",
    "migrate:down": "ts-node src/config/migrate.ts down",
    "db:migrate": "TS_NODE_PROJECT=tsconfig.scripts.json ts-node src/config/migrate.ts up",
    "vercel-build": "prisma generate && tsc",
    "postinstall": "prisma generate",
    "prisma:studio": "prisma studio",
    "seed:courses": "TS_NODE_PROJECT=tsconfig.scripts.json ts-node scripts/seed-courses.ts",
    "seed:50k": "TS_NODE_PROJECT=tsconfig.scripts.json ts-node scripts/seed-50k-users.ts",
    "simulate:activity": "TS_NODE_PROJECT=tsconfig.scripts.json ts-node scripts/simulate-user-activity.ts",
    "simulate:challenges": "TS_NODE_PROJECT=tsconfig.scripts.json ts-node scripts/simulate-challenges.ts",
    "analyze:performance": "TS_NODE_PROJECT=tsconfig.scripts.json ts-node scripts/analyze-platform-performance.ts",
    "test:full": "npm run seed:50k && npm run simulate:activity && npm run analyze:performance",
    "cleanup:fakes": "TS_NODE_PROJECT=tsconfig.scripts.json ts-node scripts/cleanup-fake-users.ts",
    "badges:generate": "node scripts/generate-badge-images.js",
    "badges:fix-urls": "node scripts/fix-badge-image-urls.js",
    "seed:realistic": "npm run seed:courses && TS_NODE_PROJECT=tsconfig.scripts.json ts-node scripts/seed-realistic-users-fast.ts && npm run badges:generate",
    "seed:fast": "TS_NODE_PROJECT=tsconfig.scripts.json ts-node scripts/seed-realistic-users-fast.ts",
    "reset:users": "npm run cleanup:fakes && npm run seed:fast",
    "seed:setup": "npm run prisma:migrate && npm run db:migrate && npm run seed:courses && npm run seed:realistic",
    "reset:admin-pass": "TS_NODE_PROJECT=tsconfig.scripts.json ts-node scripts/reset-admin-password.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/meweedan/bait-altadawul.git"
  },
  "keywords": [
    "travel",
    "tourism",
    "booking",
    "express",
    "typescript"
  ],
  "author": "Your Name <your.email@example.com>",
  "license": "MIT",
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  },
  "dependencies": {
    "@prisma/client": "^6.16.2",
    "@prisma/extension-accelerate": "^2.0.2",
    "@sentry/node": "^8.55.0",
    "@types/cors": "^2.8.19",
    "@types/dotenv": "^6.1.1",
    "@types/express": "^5.0.3",
    "@types/multer": "^2.0.0",
    "@types/node": "^24.5.2",
    "@types/pg": "^8.15.5",
    "@types/uuid": "^10.0.0",
    "@vercel/node": "^5.3.26",
    "@vercel/blob": "^1.1.1",
    "axios": "^1.12.2",
    "bcryptjs": "^2.4.3",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "dotenv": "^17.2.2",
    "ethers": "^6.16.0",
    "expo-server-sdk": "^4.0.0",
    "express": "^5.1.0",
    "express-rate-limit": "^8.1.0",
    "helmet": "^8.1.0",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.1",
    "multer": "^2.0.2",
    "node-fetch": "^2.7.0",
    "nodemailer": "^6.10.1",
    "pg": "^8.16.3",
    "puppeteer": "^23.11.1",
    "serverless-http": "^4.0.0",
    "stripe": "^18.5.0",
    "tronweb": "^6.1.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.2",
    "uuid": "^13.0.0",
    "winston": "^3.13.0",
    "zod": "^3.22.4",
    "zod-validation-error": "^4.0.2"
  },
  "devDependencies": {
    "@resvg/resvg-js": "^2.6.2",
    "@types/bcryptjs": "^2.4.6",
    "@types/cookie-parser": "^1.4.9",
    "@types/helmet": "^0.0.48",
    "@types/jest": "^29.5.11",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/morgan": "^1.9.10",
    "@types/nodemailer": "^7.0.2",
    "@types/supertest": "^6.0.2",
    "@typescript-eslint/eslint-plugin": "^6.17.0",
    "@typescript-eslint/parser": "^6.17.0",
    "artillery": "^1.7.9",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "jest": "^29.7.0",
    "lucide-react": "^0.546.0",
    "nodemon": "^3.1.10",
    "prettier": "^3.1.1",
    "prisma": "^6.16.2",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "slugify": "^1.6.6",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.1"
  },
  "prisma": {
    "seed": "ts-node ./prisma/seed.ts"
  }
}
